
d1 $ slow 2 $ off 0.25 ((# (s "xx" # gain "0.6")) . (|+| n "24")) $ slow 2 $ off 0.125 ( (# s "rash") . (|+| n "24") . (|*| gain "0.6")) $runWith 4 (zoom (0,0.25)) $ jux rev $ n (off 0.125 (subtract 12) $ off (1/3) (subtract 5) $ "[d6(3,8) g6(3,8) a6(3,8,2) f7(3,8,4)]/4") # sound "supermandolin"
  # legato 4
  # room 0.99
  # size (slow 4 saw1)
  |+| n "<0 -5 -12>"
  # orbit 1
  # cutoff 1000
  # resonance 0.3


d3 $ struct "x(3,"$ slow 4 $ runWith 4 (zoom (0,0.25)) $ jux rev $ n (off 0.125 (subtract 12) $ off (1/3) (subtract 5) $ "[d6(3,8) g6(3,8) a6(3,8,2) f7(3,8,4)]/4") # sound "vocobass"
  |+| n "12"


d2 $ stut 4 0.5 (-0.25) $ slow 2 $ runWith 4 (density 2) $ jux (|*| speed 2) $ off 0.5 (rev . (# crush 5)) $ n (run 8) # sound "micron"
  # shape 0.3

d2 silence