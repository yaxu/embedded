
d1 $ jux rev $ chop "<2 4 1>" $ chunk 4 (density 0.5) $ n ((run 8) + "0 [4 2] 1 2") # sound "micron"
  # room (slow 8 saw)
  # orbit 1
  # gain 0.9

d2 $ off 0.25 (# s "pulse") $ degrade $ jux rev $  (slow 2 $ off 0.125 (|+| n 12) $ off 0.25 (|+| n 7) $ slow 2 $ n "d7(3,8) a5(3,8,2)*2 c7(3,8) e6*2" # sound "jx")
  # gain 0.9
  # lpf (slow 8 $ saw * 2000 + 100)
  # lpq 0.2

d3 $ jux rev $ iter 4 $ loopAt 4 $ chop 16 $ sound "arc"
   # gain 1.2


d4 $ jux rev $ chop 4 $ stut "<4 2>" 0.9 0.25 $ sound "odx(3,8)"
  # gain 1


d5 $ chunk 4 (density 2 . ()) $ jux rev $ off 0.25( # speed 2) $n "0 ~ 1 2" # sound "voodoo"
  # gain 1.1
  |+| n (slow 4 $run 4)

d6 $ iter 4 $ off 0.25 (# speed "<2 3 4>") $ sound "~ [wobble wobble?]"

d7 $ degradeBy 0.5 $ jux rev $ off 0.125 (|+| n 12) $ off 0.25 (# (s "xx" # speed 2)) $ slow 2 $ n (offadd 0.25 (7) $ slow 4 "c8(3,8) [a7(6,16)]/2 f8(3,8) e7(3,8)") # sound "rash"
  # end 0.5
  # gain 0.85
  |+| n "-12"


d9 $ jux rev $ sound "gabba(3,8,<0 4>)"

d1 $ jux rev $ every 3 (chop "<4 8>") $  slow 2 $ sound "~ [~ off]"

d8 $ degrade $ chunk 4 ((|*| speed 0.5) . (slow 2)) $ jux (|*| speed "-1") $ loopAt "<8 4>" $ chop 16 $ sound "shackup"

d2 $ sometimes (|*| speed 2) $ jux (rev . (# speed 2)) $ off 0.125 (# crush 6)$ n "0 ~ 4*2 2" # sound "jvbass"
  # shape 0.7
  # lpf "<2000 5000>"
  # lpq 0.2

d9 $ iter 4 $ loopAt 2 $ chop 8 $ sound "narc"
  # n (irand 2)

d9 $ jux rev $ iter 4 $ loopAt "<4 8>" $ chop 8 $ sound "liq"
d9 silence

d1 $ off 0.25 (# crush 4) $ slow 2 $ n (run 16 + (irand 4)) # sound "amencutup"
  # gain 1.1

d5 $ jux rev $ iter 4 $ up "0 ~ [7 3] ~ 2" # sound "arpy"

d2 $ sound "~ wobble*3"
  # crush "<4 3>"

d1 $ iter 4 $ sound "{future(3,8) future:2*2, ~ ~ cp8:3}"
  # room 0.8
  # size 0.4
  # orbit 1
  # speed 2
