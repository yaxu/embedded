d1 $ off 0.25 (|*| (crush 5 |*| speed 1.5)) $ n "{0 ~ 2 3, [~ 7] ~ ~}" # sound "<mt8 ht8>"

d2 $ jux ((0.5 <~)) $ slow 2 $ off 0.25 (|*| up 7) $ substruct "x x/2" $ up (offadd 0.125 12 $ "0(3,8)" + "<0 5 [7 12] -12>") # sound "sid:3"
  # orbit 1 # room 0.4 # size 0.6
  # cutoff (slow 4 $ sine1 * 1000 + 1000)
  # resonance 0.3
  |*| speed 0.5

e3 