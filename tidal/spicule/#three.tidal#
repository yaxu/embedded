-- hmm.. maybe some toms

d1 $ sound "lt ~ ht . mt ht"
  # n (slow 16 $ run 16)
  # speed "[1,1.5]"

d2 $ stut 4 0.9 2 $ (density 2) $ (sound "lt sd8 . bd lt")
  # n (slow 16 $ run 16)
  # speed "1.5"
  # gain "0.95"

--

d3 $ (density 2 $ brak $ n "58 [2 2?]" # s "jvrave")
  # shape "0.5"
  # speed "2"
  # resonance "0.3"

d4 $ sound "funky"

d6 $ whenmod 8 4 (substruct "x/2 x/4") $ struct "x ~ [x x?]" $ n (slow 4 $ run 16) # sound "jvr"

d5 $ stut 2 0.25 (-1) $
  every 3 (# (cutoff "10000" # resonance "0.3")) $ n "~ [16 4 4*2 16]/4 ~ {0 12 24*2 [24 36] 0}%1" # sound "pulse"
  # gain "0.9"

d7 $ stut 6 0.25 1 $ every 2 (# (cutoff "200" # resonance "0.2" # room "0.9" # orbit "1")) $ iter 4 $ n "~ [4 4 4 16]/4 ~ [0 12 24]/3" # sound "pulse"
  # gain "0.9"

--

d1 $ every 6 (0.25 <~) $ sometimes (stut 2 0.25 (-0.25)) $ slow 2 $ n "{4 5 4 5, 8 ~ ~} . [1, 7] . {7 [4 ~ 4 ~], 8 ~ {~ ~ ~ 9}%1} . [1,7] 5" # sound "jvr"
  # crush "4"

d2 $ every 2 (superimpose (struct "x . x" . (|+| n "12") . (# (orbit "1" # room "0.8")))) $ n (slowspread (offadd 0.125) [7,9,12,24] "5 5 . 8 12") # sound "pulse"
  # crush "5"

d3 $ superimpose (iter 4) $ slow 2 $ n "c9 [~ ~ ~ f8]" # sound "rash"

