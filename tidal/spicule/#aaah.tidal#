let orb0 = (# (orbit "0"
               # room (slow 4 saw1))
           )
    orb1 = (# orbit "1")
    pad = (whenmod 16 8 (jux rev) $ up (every 4 ((+ 3) <$>) $ "0 [~ 5 ~] ~ 8") # sound "pad:3")
          # end "0.15"
          # size "0.9"
          # gain "0.9"
    gab = sound "[gabba(3,8), ~ [feel:6 feel:3]]"
    gab1 = whenmod 8 4 (iter 4) $ n (run 8) # s "gabba"
           # speed "[2,1]"
           # gain "0.9"
           # orbit "1"

  
do cps 1.1
   let now'' = now'
   d10 $ (nextSam (now''+1)) ~>
     seqP [(0, 32, orb0 pad),
           (8,32, orb1 gab),
           (16,32, orb1 gab1)
          ]

d3 $ 



