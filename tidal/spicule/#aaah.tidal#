  # speed "0.8"
  # gain "1"
  # orbit "0"

d8 $ runWith 4 (|*| speed "2") $ jux rev $ sound "jvbass:1(5,8)"
  # gain "1.1"
  # speed "2"
  # resonance "0.4"
  # cutoff (slow 4 $ every 4 rev $ scale 300 2000 saw1)

d9 $ whenmod 4 1 (const silence) $ (jux rev $ slowspread (\x -> zoom (0,x)) [0.5,0.25,(1/3), 0.125,1] $ chop 32 $ sound "redflag")
  # gain "0.9"
  # speed ((*2) <$> "0.9")
  # resonance "0.3"
  # cutoff (slow 4 $ every 4 rev $ scale 3000 4000 saw1)
  # vowel "a"

d1 $ runWith 4 (density 2) $ slow 2 $ stut 8 0.7 (-0.5) $ slow 4 $ up "{[0 ~ 3] 7 [3 12] [2 5]}" # sound "wobble"
  |*| speed "[2,3,1]"
