cps 0.8


-- face arpeggio

d1 $ degradeBy 0.7 $ sometimes (# speed "2") $ jux rev $ n ((subtract 0) <$> (offadd 0.25 12 $ slow 4 $ offadd 0.125 7 $ "f8(3,8) a7(3,8) c7*2 e8(3,8,2)")) # sound "[jx,xx]"
  # cutoff (slow 4 $ scale 400 2000 sine1)
  # resonance "0.2"
  # end "0.05"
  # gain "0.85"

d2 $ substruct "x x/2" $ (jux (iter 4) $ sound "[bd8(3,8) ~, ~ sd8]")
  # delay "0.3" # delaytime "0.06"
  # speed "[2]"


d3 $ degradeBy 0.8 $ jux rev $ (every 4 (density 2) $ sound "rash([5 3]/2,8,[0 2 3 4]/4)") # n ( "[f8 a7 c7 e7]/4")
  # cutoff (slow 4 $ scale 400 2000 sine1)
  # resonance "0.2"

d4 $ jux ((1 <~) . (|*| speed "2") . density 2) $ iter 4 $ loopAt 4 $ chop 8 $ sound "arc"

-- comb delay drop

d1 $ stack [sound "{pulse:40 {pulse:38 pulse:37 pulse:36 pulse:35}%1 ~}%2",
            brak $ sound "[feel feel:3, feel:1*4]"
            # gain "0.9"
           ]
  # delay "0.9"
  # delaytime (slow 4 $ scale 0 0.25 saw1)
  # delayfeedback "0.99"
  # orbit "1"
  # speed "[[0.5 0.75]/2,1]"

d2 $ jux (iter 4) $ sound "pulse:11(3,8)"
  # orbit "0"
  # shape "0.5"

d3 $ jux rev $ slowspread (chop) [8,4,2] $ sound "feel:2 bd8:6*8"

-- sine nudge chop

d1 $ stack [brak $ ((slowspread (chop) [32,64,128,16] $ n "0 5 [~ 5] 3" # sound "feel")),
            chop 62 $ sound "drum:0 [drum:3 drum:2] [~ drum:1*2] drum:1"
            # shape "0.5"
           ]
  # orbit "1" # delay "0"

d2 $ n "d7" # sound "rash"
  # shape "0.4" # crush "5"
  # speed


