

d1 $ slow 2 $ runWith 4 (density 2) $ (jux rev $ sound "[bd8:2(3,8,[0 2]/2), [[sd8 sd8:2]/2](5,8,2)]")
  # gain "1.1"
  # orbit "1"

d2 $ jux rev $ every 2 (density 1.5) $
  weave 16 (n (run 8) # sound "arpy")
  [vowel "a e i",
   speed "1 2 ~ 3",
   vowel "a i u*2 y",
   speed "0.5 1 ~ 0.25"
   ]

d3 $ sound "funky"

d3 $ jux (iter 4) $ sound "~ . sd8:3*2 ~ cp8*2 ~"

d4 $ sound "~ ravebass:10*2"