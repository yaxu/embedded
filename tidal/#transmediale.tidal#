cps 1.1

-- this is aalleexx

d1 $ jux rev $ every 4 (chop 128) $ n "0 1 ~ 1" # sound "{bd8, lt8 ht8 mt8 ~}"

d2 $ off 0.25 (# vowel "a [e i]") $ up "0(3,8) [7 3]" # n "0" # sound "bass3"
  # shape "0.3"
  # gain "0.9"

d4 $ every 2 (density 2) $ jux (iter 4) $ slow 2 $ sound "bd ~ mt8 bd . bd sd8:2 bd"

d1 $ jux rev $ n (run 16) $ sound "voodoo"
  # speed "[1,1.1,2]"
  # gain "1.1"

d1 $ rev jux (|+| n "[7 3 12]/2") $ every 4 (slow 1.5) $ off (0.125) (|+| n "7") $ off 0.25 (|+| n "12") $ slow 2 $ n "[d7(3,8) a7(3,8) f7(3,8) e7(3,8)]/4" # sound "rash(3,8)"
  # vowel "a"
  # cutoff (slow 16 $ (scale 4000 7000 saw1))
  # resonance "[0.1 0.2]*3"

d2 $ every 2 (slow 2) $ n "0 ~ 2 . 16 2" # sound "jvbass"
  # orbit "1"
  # room (slow 4 saw1)
  # size "0.8"
  # speed "[1,2]"

d3 $ jux (rev . (# speed "2"))  $ sound "amencutup*8"
  # n (palindrome $ run 16)

d4 $ sound "bd8:3*4?"
  # shape "0.7"
  # crush "4"
