
d1 $ (jux rev $ off 0.5 (# (s "supersaw" # speed "2")) $ off 0.25 (rev . (# s "supermandolin")) $ n (slow 2 $ off 0.125 (+7) $ slow 2 $ "[d6(3,8) c6(3,8)] a5(3,8) es5(3,8) f6(5,8)") # s "[pulse]") |+| n "<0 12 7>"
