d1 $ jux (rev . chop 8) $ sound "micron(3,8)" # n (slow 4 $ run 8)

d2 $ jux rev $ chop 8 $ struct "x(3,8)" $ slow 4 $ n (run (8)) # sound "claus"

d3 $ jux ((# speed "2") . rev) $ n "0 ~ ~ 5 6 ~ 5 ~" # sound "micron"

d4 $ sound "micro"

d1 $ iter 4 $ sound "arpy:0 arpy:1 arpy:2 arpy:3"

d1 $ do t <- slow 8 (sine1 * 0.05)
        off 0.125 (|*| up "<7 0 -5 -12>") $ off 0.25 (|*| up 12) $ sound "rash:40*2 arpy:2? arpy:3 ~"
  # speed "[0.75,0.5]"
  # hcutoff (slow 4 $ sine1 * 1000 + 1000)
  # hresonance 0.3

d1 $ off 0.125 (|*| up 7) $ off 0.25 (|*| up 12) $ sound "rash:40 arpy:2 arpy:3 ~"

d1 $ sound "rash:40"
