cps 0.85

d1 $ slow 2 $ jux rev $ within (0.75,1) (|+| n 12) $ (off 0.25 (# (s "rash" |+| n "12")) $ off (1/3) (# (crush 6 |+| n "<7 7 5 5>")) $ chop 2 $ n "d6*4" # sound "jx")
  # speed 2
  # cutoff (slow 2 $ scale 500 1000 saw)
  # resonance (scale 0.1 0.3 saw)
  # orbit 1
  # room 0.9
  |+| n (palindrome "<0 0 2 2 3 3 5 5>")

d2 $ chunk 4 (density 2 . (# speed 2)) $ juxBy 0.5 rev $ iter 4 $ n ("0-7" + 4) # sound "micron"
  
d3 $ sound ""