
d1 $ degradeBy 0.3 $ off 0.25 ((struct "x(3,8)") . (# (s "superpiano" # gain 0.7))) $ jux rev $ slow 2 $ slow 2 $ off 0.125 (|+| n (-12)) $ off 0.25 (|+| n "7") $ n "c(3,8) e*3 f(3,8) g(3,8)" # sound "supermandolin"
  # lpf (slow 4 $ sine * 3000 + 400)
  # lpq 0.2
  # room 0.2
  # legato "<2 4>"
  # gain 1.1


d3 $ jux' [id, rev, iter 4, slow 2] $ chunk 4 (hurry 2) $ n "0 [~ 1] 2 3" # sound "micron"
  # gain 0.75
  # speed 2

d2 $ (chunk 4 (hurry 0.5) $ jux' [id,rev,iter 4] $ loopAt 4 $ chop 16 $ sound "paper")
  # gain 0.8
  |+| pan saw


d3 $ chunk 4 (hurry 2) $ jux' [id, (# speed 4), (vowel "<a i*2 u*3 o*4>" #), (rev . chop 8), (off "<0.25 0.5>" (# crush "<5 5 5 3>") )] $ chunk 4 (hurry 0.5) $sound "<bd(3,8) bd:3(5,8)>"

d2 $ off "<0.25 0.5 0.75>" (# crush 5) $  n "d6 [~ g7 ~ e5]" # sound "rash"
  # gain "0.8"
  