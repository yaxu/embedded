
d1 $ do x <- slow 8 saw
        juxBy (x) rev $ linger "<1 0.5 0.75 0.5>" $ n "0 ~ 8 [~ 5] 0 [~ 9] ~ [7 5*2]" # sound "micron"
  # room 0.7
  # size 0.4
  # speed "[2,1.5]"
  # orbit 1

d2 $ degradeBy "<0.25 0.5>" $ (juxBy 0.25 rev $ (chop 64 $ n "d6 c5(3,8)" # sound "rash"))
  # gain 1.2
  # shape 0.3
  # lpq "<0.3 0.34>"
  # lpf (density "<1 2 3 4>" $ sine * 2000 + 1000)
  |+| n "<0 7 12 7>"

d4 $ jux rev $ off "<0.25 0.5 0.125>" (# s "pulse") $ n "d7(3,8) [c6 ~ c6]" # sound "jx"
  # gain 1.4
  # lpq "<0.3 0.34>"
  # lpf (density "<1 2 3 4>" $ sine * 2000 + 1000)



d3 $ jux ((# crush 4) . ((|*| speed 1.5) . (fast 1.5))) $ iter 4 $ loopAt 4 $ chop 16 $ sound "shackup"
  # gain 1.5