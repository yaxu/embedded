-- hello watcher!

d1 $ jux rev $ linger "<1 0.5 0.75 0.5>" $ n "0 ~ 8 [~ 5] 0 [~ 9] ~ [7 5*2]" # sound "micron"
  # room 0.7
  # size 0.9
  # speed "[,1.5]"
  # orbit 1

d2 $ (juxBy 0.25 rev $ (chop 64 $ n "d6 c5(3,8)" # sound "rash"))
  # gain 1.2
  # shape 0.6
  # lpq "<0.3 0.34>"
  # lpf (density "<1 2 3 4>" $ sine * 2000 + 1000)
  |+| n "<0 7 12 7>"