
d1 $ (jux (rev) $ n "e6 e7 [g7 g6]" # s "jx")
  # gain "1.3"
  # cutoff (slow 4 $ scale 100 2000 saw1)
  # resonance "0.2"
  # speed "[0.5,1]"

d2 $ (jux (rev) $ n "{e8 e6 e9 e6 e8}%1 e7 [g7 g6]" # s "rash")
  # gain "1.2"
  # cutoff (slow 4 $ scale 100 2000 saw1)
  # resonance "0.2"

d3 $ jux rev $ runWith 3 (density 2 . (# speed "2")) $ n "{[1 4 ~] 5, 7*2}" # s "jvrave"
  # gain "1.1"
d2 silence
d4 $ jux rev $ off (1/3) (|+| n "7") $ n "e7(4,6)" # sound "rash" |+| n "{0 5 7}%1"
  # gain "1.2"

d5 $ runWith 4 (density 2) $ n "0*4" # s "jvrave"
  # shape (slow 8 $ scale 0.3 0.9 sine1)

d6 $ substruct "x [x/2 x/2]" $ vowel "{a e i o u}%2" # n "e7" # s "jx"
  # gain "1.4"

d7 $ jux rev $ n (offadd 0.25 7 $ slow 2 "~ e7(3,8) ~ b7(3,8)") # s "jx"
  # gain "1.4"