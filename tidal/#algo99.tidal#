d1 $ degrade $ off 0.25 (# (s "superpiano" # gain (0.9))) $ jux rev $ slow 4 $ off 0.25 (|+| up 12) $ off 0.125 (|+| n 7) $ n "c(3,8) e(3,8,4) f*2 g*3"
  # sound "supermandolin"
  # legato 2

d2 $ jux rev $ iter 4 $ loopAt 4 $ chop 32 $ sound "paper"

d3 $ chunk 4 ((# speed 0.5) .slow 2) $ jux (rev . chop 4) $ slow 2 $ off 0.25 (# crush 3) $ n "0 .. 3" # sound "micron"
  # gain "1"

d2 $ slow 2 $ iter 4 $ jux (0.25 <~) $ loopAt 2 $ chop 16 $ sound "<os os:2>"


d4 $ every 4 (|*| up "7") $ n "2(3,8)" # sound "stabs"
