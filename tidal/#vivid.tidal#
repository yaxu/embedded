(click1, _) <- superDirtSetters getNow

click1 silence

click1 $ n "0 1 1 1" # sound "casio"
  # orbit "2"
  # gain "1.3"

cps 0.9



d1 $ degrade $ (sometimes (# speed "2") $  jux rev $  slow 2 $ n (offadd 0.125 12 $ offadd 0.25 7 $ slow 2 $  "[[f7 g7](6,16)]/2 d7(3,8) e7(3,8) a5(3,8)") # sound "rash")
  # cutoff (slow 4 $ scale 500 5000 sine1)
  # resonance "0.3"

d2 $ runWith 4 (density 2) $ jux rev $ slowspread (chop) [4,8,16] $ sound "[gabba, wobble](3,8)"


d3 $ sound "~ off"

d1 $ runWith 4 (density 2) $ every 4 (# speed "0.5") $ slowspread (chop) [128, 64, 32] $ sound "bd [~ drum] . feel mt ~ drum"
  # delay "0.9"
  # delaytime (every 4 (rev) $ scale 0.01 0.001 sine1)
  # delayfeedback "0.9"
  
d2 $ sound "drum(3,8)"  

d3 $ every 2 (slow 2) $ jux rev $ weave 16 (n (run 8) # sound "arpy*8")
  [vowel "a i u i",
   vowel "a o u",
   speed "1 2 3",
   speed "2 0.5 1 1.5"
  ]
  # orbit "1"
  # gain "1.2"
  |*| speed "[0.5,1]"
y
d4 $ every 4 (# speed "0.5") $ iter 4 $ jux rev $ loopAt 2 $ chop 8 $ sound "arc"
  # shape "0.6"

d5 $ every 2 ((|*| speed "2")) $ jux (rev) $ every 2 (|+| up "12") $ up "0 7 ~ 3" # sound "bass3"
  # shape "0.3"

d2 $ brak $ sound "bd*2 mt ~ mt bd rs ~ sn:2"

d3 $ jux rev $ iter 4 $ loopAt 8 $ chop 32 $ sound "liq"
  # gain "0.9"
  # cutoff (scale 100 8000 sine1)
  # res 

d4 $ jux rev $ sound "{bd bd*4, sn:3 gabba*2 kurt*2}"
