
d1 $ chunk 4 (slow 2) $ off 0.125 (# crush 5) $
  off "<0.25 0.125>" (# speed 2) $
  slow 2 $ sound "bd [~ micron] wobble micron:4"
  # room 0.7
  # size 0.7
  # orbit 1

d2 $ degradeBy 0.7 $ off 0.25 (# s "pulse") $ jux rev $ slow 2 $ n (off 0.125 (+12) $ off 0.25 (+7) $ "[d7 e6] [<f7 g7> a6(3,8)]") # sound "rash"

d3 $ every 2 ((|+| speed 0.5) . slow 2) $ chunk 4 (density 2) $ loopAt 1 $ chop 128 $ sound "arc"
  # gain 1.3

d4 $ off 0.125 (# speed 2) $ every 3 ((|*| speed 2) . fast 2) $ jux rev $ n (run 8) # sound "micron"

d5 $ degradeBy 0.4 $ off 0.25 (# s "jx") $ jux rev $ sometimes (|+| n 12) $ slow 2 $ n (off 0.125 (+12) $ off 0.25 (+7) $ slow 2 $ "c7(3,8) a7(3,8,4) f7(3,8,2) e7*2") # sound "xx"
  # gain 1

d6 $ jux rev $ chunk 4 (slow 2) $ loopAt 1 $ chop 16 $ sound "os"
  # gain 1.3
