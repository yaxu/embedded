let jux f p = stack [p |+| pan (pure (5/8)), f $ p |+| pan (pure (1/8))]


d1 $ jux (|*| speed "0.5") $ slowspread (chop) [1] $ jux' [id, ((|+| n "5") . (0.25 <~)), density 2 . (# speed "2")] $ sound "mt8:2(<3 3 5>,8,[0 2]/2)"
  # gain "0.6"

-- seqPLoop no 9

d1 $ seqPLoop [
                 (0,8,
                  chop 16 $ n "11(<4 2 6 2>,9)" # sound "jvr"
                  # cut "-1"
                  # speed "2"
                  # accelerate "-0.1"
                  # cutoff (slow 4 $ scale 1000 10000 sine1)
                  # resonance "0.2"
                 )
                ]
  # orbit "1"
  # room (slow 8 saw1)
  # size "0.3"

d2 $ sound "bd . sd8:5*2 . cp8"

