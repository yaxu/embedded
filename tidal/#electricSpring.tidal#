let jux f p = stack [p |+| pan (pure (5/8)), f $ p |+| pan (pure (1/8))]

-- number 9

d1 $ slow 2 $ jux rev $ off (1%4) (# speed "0.75") $ off (1%8) ((# speed "1.2") . (# cutoff "1000")) $
  n "<5 7 [5 9] 8>(<2 3 4 5>,8)" # sound "jvr"
  # crush "5"
  # cut "-1"

-- mismatch step tempo

-- tabby

d2 $ tabby 8
  (n (run 8) # sound "voodoo" # speed "0.5" # cutoff "1000" # resonance "0.3")
  (n (run 8) # sound "jvr" # orbit "1" # size "0.3" # room "0.3" |+| n "[6 4]/2")


d3 $ do x <- "<1%4 1%8 1%16>"
        jux' [id, rev. (|*| speed "2") . iter 4, density 1.5] $ slow 4 $ off x (|+| n "7") $ n "c6 ~ . ~ c7 . [~ e7] d6 . a7 c7" # sound "rash"

d4 $ jux' [id, rev, (0.5 <~)] $ runWith 4 (slow 2 . (|*| speed "0.5")) $ iter 4 $ loopAt 8 $ chop 32 $ sound "shackup"

d5 $ slow 2 $ off 0.25 (|+| n "12") $ off 0.125 (|+| n "7") $ density 2 $ struct "<x(3,8) x(5,8)>" $ slow 4 $ (n "c7 a8 <g7 f7> e7" # sound "xx")
  # cutoff (slow 4 $ scale 1000 3000 saw1)
  # resonance "0.3"
  

