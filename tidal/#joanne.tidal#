
d1 $ jux rev $ chunk 4 (hurry 2) $ off 0.25 (|+| (speed 2 # crush 5)) $ sound "sd8:2(3,8)"
  |+| speed (slow 4 sine)

d2 $ sound "~ sd8:3"

d3 $ every 3 (fast 2) $ n "0 .. 3" # sound "micron"
  # speed 2
  # gain 0.9


d4 $ degrade $ jux rev $ slow 2 $ (off 0.125 (|+| n 7) $ off 0.25 (|+| n 12) $ n "c4(3,8) e3 <f3>(3,8) g4" # sound "supermandolin")
  # orbit 1
  # legato 4
  # crush 5
  # gain 1.3
  # lpf (slow 4 $ sine * 3000 + 1000)
  # lpq 0.1