
d1 $ (off 0.25 (# crush 5) $ jux rev $ sound "bd(3,8,[2, 4])")
  # (slow 8 $ room saw1 # size saw1)
  # orbit 1

d2 $ off 0.125 ((|*| speed 2) . slow 2) $ off 0.125 ((|*| speed 1.5) . rev) $ chop "[8 4]/2" $ sound "newnotes"
  # room 0.9 # size 0.95

d3 $ chunk 4 (# crush 4) $ slow 2 $ off 0.125 ((|*| speed 2) . slow 2) $ off 0.125 ((|*| speed 1.5) . rev) $ chop "[2 4]/2" $ sound "newnotes [newnotes:2 newnotes:3]"
  # orbit 2
  |*| speed "0.5"
  # resonance 0.3
  # lpf 3000
