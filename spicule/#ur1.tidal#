d1 $ slow 2 $ off 0.25 (|-| n 4) $ n ((run "[8 16] <4 8 4 2>") + 7) # sound "micron"
  # speed 2

d2 $ (slow 2 $ degradeBy "0 0.3" $ off "<0.25 0.5 0.125>" (# (s "xx" # speed 2 # legato 0.5)) $ jux (rev . (|+| n "<7 7 7 7 12>")) $ off 0.25 (|+| n 7) $ n "<c7(3,8,<0 4>) a7(3,8) f7(3,8) e7(3,8)>" # sound "rash")
  # gain 1.1
  # legato 2
  # orbit 1
  # size 0.75
  # room 0.8
  # cutoff (slow 4 $ saw1 * 1000 + 3000)
  # resonance "<0.2 0.3 0.4>"

