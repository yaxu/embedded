



d1 $ ur' 16
  "[simple:wake complex:wake simple:wake shadow:wake, tune]"
  [("simple", sound "bd*4"
   ),
   ("complex", off 0.25 (# crush 4) $ sound "micron:5(3,8)"
   ),
   ("shadow", off 0.25 (# s "drum:2") $ off 0.25 (# crush 4) $ sound "<micron:5>(3,8, <0 2 4>)"
   ),
   ("tune", n "e7 ~ g7" # s "jx"
            
   )
  ]
  [("wake", (# (room saw # size saw # orbit 1)))
  ]
  
