
d1 $ every 2 (|*| speed "0.5") $ jux ((# speed "2")) $ chop "<8 4 2>" $ n (every 4 (+2) $ struct "x(3,8,[0 2])" $ run 4) # sound "micron"
  # delay 0.8
  # delaytime (sine1 * 0.01)
  # delayfeedback "0.7"
  # orbit "1"

d2 $ n (off 0.125 (+12) $ off 0.25 (+7) $ slow 4 $ "c7(3,8) a7(3,8) f6*2 e7*2") # sound "rash"
  # gain "1.2"

d3 $ jux (0.25 <~) $ runWith 4 (density 2) $ density "1 <0.5 2>" $ jux rev $ n "0 3(3,8) 1 ~" # sound "future"
  # gain "1.1"

d4 $ degrade $ slow 2 $ jux rev $ substruct "x x/2" $ n (every 2 (+7) $ off 0.25 (+12) $ "c6 a6(3,8) f6 e7") # sound "pulse"
  # cutoff (slow 8 $ sine1 * 2000 + 300)
  # resonance 0.2
  # gain "1.1"

d5 $ iter 4 $ loopAt 2 $ chop 8 $ sound "arc"
  # gain "1.6"

d6 $ iter 4 $ loopAt 4 $ chop 16 $ sound "repetition"
  # gain "1.4"
  # n (irand 16)

d7 $ stut 3 0.7 0.03 $every 3 (0.25 <~) $ every 2 (density 2) $ sound "odx(3,8,[0 2]/2)"
  # gain "1.4"
  # cutoff 3000

d8 $ jux rev $ iter 4 $ runWith 4 ((|*| speed "2") . density 2) $ loopAt 4 $ chop 16 $ sound "liq"
  # gain "1.4"

d9 $ jux rev $ loopAt 2 $ chop 16 $ sound "shackup"
  # gain "1.3"
  # cutoff "[1000 500]/2"


d1 $ up (off 0.125 (+12) $ off 0.25 (+7) $ "0 [7 2] 2 12") # sound "latibro"
  # gain 1.2
  # crush "6"

d2 $ jux rev $ off "<0.25 0.5>" (# crush 5) $ n "0 [4 5]" # sound "micron"
  # gain 1.5