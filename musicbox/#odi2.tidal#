

d1 $ sometimes (|*| speed "4") $ jux rev $ slow 2 $ n (offadd 0.25 12 $ offadd 0.25 7 $ slow 2 $ (+12) <$> (struct "x(3,8,[0 2]/2)" p)) # sound "xx" # gain "0.8"
  # room "0.8"
  # size "0.9"
  # cutoff (slow 4 $ scale 1000 3000 sine1)
  # resonance "0.3"

d2 $ jux rev $ runWith 4 (density 2) $ sound "[bd8:4(3,8), ~ [mt8 ht8:3]]"
  # orbit "1" # gain "1.2" # speed "[2 3]/2"