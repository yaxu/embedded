let oscport = 9000
    startcps = do s <- udpServer "127.0.0.1" oscport
                  oscLoop s
    oscLoop s cps = do m <- recvMessage s
                       act m cps
                       oscLoop s cps
    act (Just (Message "/notes" xs)) cps =
      do putStrLn $ show xs
         return ()
    act _ _ = return ()

